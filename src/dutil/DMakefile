.ifos AmigaOS
.include /DMakefile.inc
.else
.include ../DMakefile.inc
.endif

SRCS= bintohex.c fdtolib.c fdtopragma.c istrip.c lbmake.c \
        libtos.c makeproto.c romable.c hunks.c uprev.c

EXES= $(SRCS:"*.c":"$(OD)*")
EXESD= $(SRCS:"*.c":"$(BIN)*")

all: $(EXES)

$(EXES) : $(SRCS)
        $(CC) $(CFLAGS) %(right) -o %(left)

install:
        $(INSTALL) $(EXES) $(BIN)

clean:
        -$(RM) $(EXES)

